                                    <span class=" fw-bold">Technologia druku: </span>
                                    <span>@Model.PrintTechnology</span>

                                    <br />

                                    <span class=" fw-bold">Maksymalna rozdzielczość druku: </span>
                                    <span>@Model.MaxPrintingResolution</span>

                                    <br />

                                    <span class=" fw-bold">Obsługiwane typy nośników: </span>
                                    <span>@Model.SupportedMediaTypes</span>

                                    <br />

                                    <span class=" fw-bold">Obsługiwane formaty nośników: </span>
                                    <span>@Model.SupportedMediaFormats</span>


                                    <br />

                                    break;

                                case ProductType.Smartphone:
                                    <span class=" fw-bold">Producent: </span>
                                    <span>@Model.Producer</span>

                                    <br />

                                    <span class=" fw-bold">Tylnia kamera: </span>
                                    <span>@Model.BackCamera</span>

                                    <br />

                                    <span class=" fw-bold">Drukowanie obustronne: </span>
                                    <span>@Model.FrontCamera</span>

                                    <br />

                                    <span class=" fw-bold">Technologia druku: </span>
                                    <span>@Model.ScreenDiagonal</span>

                                    <br />

                                    <span class=" fw-bold">Maksymalna rozdzielczość druku: </span>
                                    <span>@Model.Processor</span>

                                    <br />

                                    <span class=" fw-bold">Obsługiwane typy nośników: </span>
                                    <span>@Model.RamSize</span>

                                    <br />

                                    <span class=" fw-bold">Obsługiwane formaty nośników: </span>
                                    <span>@Model.OperatingSystem</span>


                                    <br />

                                    break;


                                case ProductType.Televisor:
                                    <span class=" fw-bold">Producent: </span>
                                    <span>@Model.Producer</span>

                                    <br />

                                    <span class=" fw-bold">Częstotliwość odświerzania ekranu: </span>
                                    <span>@Model.ScreenRefreshRate</span>

                                    <br />

                                    <span class=" fw-bold">Rozdzielczość: </span>
                                    <span>@Model.Resolution</span>

                                    <br />

                                    <span class=" fw-bold">Przekątna ekranu: </span>
                                    <span>@Model.ScreenDiagonal</span>

                                    <br />

                                    <span class=" fw-bold">Porty zewnętrzne: </span>
                                    <span>@Model.ExternalPorts</span>

                                    <br />

                                    <span class=" fw-bold">Smart TV: </span>
                                    <span>@(Model.HasSmartTv == true ? "Tak" : "Nie")</span>

                                    <br />
                                    if (Model.HasSmartTv == true)
                                    {
                                        <span class=" fw-bold">Obsługiwane formaty nośników: </span>
                                        <span>@Model.OperatingSystem</span>
                                    }


                                    <br />

                                    break;
                            }

                        </div>
                    </div>
                    <div style="float: right">
                    </div>
                </div>
            </div>
        </div>
        break;
